{
    "manifest_version": 2,
    "name": "Github1s Chrome",
    "version": "0.0.0",
    "description": "This is a simple extension that makes it easy to jump back and forth between github1s and github.",
    "author": "GHOST#7524 | github.com/ghostdevv",
    "browser_action": {
        "default_title": "Github1s Chrome extension"
    },
    "background": {
        "scripts": ["src/background.js"]
    },
    "icons": {
        "16": "assets/16-disabled.png",
        "32": "assets/32-disabled.png",
        "64": "assets/64-disabled.png",
        "128": "assets/128-disabled.png"
    },
    "permissions": ["tabs"]
}
